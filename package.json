{
  "name": "frax-price-feed",
  "version": "1.0.0",
  "description": "a price feed that queries the Ethereum blockchain for on-chain events which update the FRAX, FXS, and collateral prices",
  "main": "src/index.js",
  "scripts": {
    "start": "tsc && nodemon dist/index.js",
    "server-graphql": "tsc && nodemon dist/server/server-graphql.js",
    "server-rest": "tsc && nodemon dist/server/server-rest.js",
    "deploy-ropsten": "tsc && nodemon src/truffle/deploy/ropsten/deploy-ropsten.js",
    "test": "npm test",
    "tsc": "tsc"
  },
  "keywords": [
    "ethereum",
    "frax",
    "stablecoin",
    "defi"
  ],
  "author": "Jason Huan <sino@ucla.edu> (github.com/jasonhuan)",
  "license": "ISC",
  "dependencies": {
    "@chainlink/contracts": "0.0.10",
    "@types/express": "^4.17.7",
    "@types/node": "^14.10.1",
    "@types/web3": "^1.2.2",
    "apollo-server-express": "^2.17.0",
    "bignumber.js": "^9.0.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "mongoose": "^5.10.2",
    "node-rest-client-promise": "^3.1.1",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "randomcolor": "^0.6.2",
    "require-from-string": "^2.0.2",
    "solc": "^0.6.12",
    "truffle-contract-size": "^2.0.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2",
    "util": "^0.12.3",
    "web3": "^1.2.11",
    "web3-eth-contract": "^1.3.0",
    "web3-utils": "^1.3.0"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "^0.5.6"
  }
}
